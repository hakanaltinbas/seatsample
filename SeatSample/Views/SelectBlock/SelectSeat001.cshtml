@section Scripts{
    <script src="~/Scripts/jquery.maphilight.min.js"></script>
    <script src="~/Scripts/easyTooltip.js"></script>
    <script src="~/Scripts/jquery.rwdImageMaps.min.js"></script>
    <script src="~/Scripts/jquery.signalR-2.2.1.min.js"></script>
    <script src="~/signalr/hubs"></script>
    <script type="text/javascript">
        $(function () {
            $('#venue').maphilight({ strokeColor: 'f08b14', fillOpacity: '0.3' });
            $('map > area').easyTooltip();
            $('img[usemap]').rwdImageMaps();

            var seat = $.connection.seatHub;
            seat.client.addNewMessageToPage = function (id, status) {
                if (status == "s") {
                    if ($("#" + id).hasClass("z")) {
                        $("#" + id).removeClass("z").addClass("s");
                    }
                }
                else if (status == "z") {
                    if ($("#" + id).hasClass("s")) {
                        $("#" + id).removeClass("s").addClass("z");
                    }
                }
            };

            $.connection.hub.start().done(function () {
                $('.ye').click(function () {


                    if ($(this).hasClass("s")) {
                        $(this).removeClass("s").addClass("y");
                        seat.server.send($(this).attr("id"), "z");
                    }
                    else if ($(this).hasClass("y")) {
                        $(this).removeClass("y").addClass("s");
                        seat.server.send($(this).attr("id"), "s");
                    }

                });
            });
        });

    </script>
}